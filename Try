    <script>
        function postContactToGoogle() {
            var email = $('#Email').val();
                $.ajax({
                    url: "https://docs.google.com/forms/d/1QWj7wnkjSEl8QazL1WbCxqeC0z3rnhPTVpQkWp4Ku0I/formResponse",
                    data: { "entry_639894026": email },
                    type: "POST",
                    dataType: "xml",
                    statusCode: {
                        0: function () {
                            window.location.replace("thankyou.html");
                        },
                        200: function () {
                            window.location.replace("thankyou.html");
                        },
                        201: function () {
                            window.location.replace("thankyou.html");
                        }
                    }
                });
        }
    </script>